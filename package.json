{
  "name": "@hathway/mparticle-capacitor",
  "version": "1.2.1-beta.6",
  "description": "mParticle Capacitor Plugin",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "HathwayMparticleCapacitor.podspec"
  ],
  "author": "Anthony Escobar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hathway/mparticle-capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/hathway/mparticle-capacitor/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "start": "ng serve",
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- autocorrect --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api MParticleCapacitorPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular/animations": "~13.1.0",
    "@angular/cli": "~13.1.3",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@capacitor/android": "^4.0.1",
    "@capacitor/core": "^4.0.1",
    "@capacitor/docgen": "^0.2.0",
    "@capacitor/ios": "^4.0.1",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "@types/jasmine": "~3.10.0",
    "@types/mparticle__web-sdk": "^2.14.0",
    "@types/node": "^16.11.0",
    "@vitest/coverage-c8": "^0.29.1",
    "eslint": "^7.11.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "~2.2.0",
    "prettier-plugin-java": "~1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "rxjs": "~7.4.0",
    "swiftlint": "^1.0.1",
    "tslib": "^2.3.0",
    "typescript": "~4.4.2",
    "vitest": "^0.29.1",
    "zone.js": "~0.11.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^4.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "dependencies": {
    "@mparticle/web-sdk": "^2.16.2"
  }
}
